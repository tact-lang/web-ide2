{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tact",
      "camelcase": "Tact",
      "scope": "source.tact",
      "file-types": ["tact"],
      "injection-regex": "^tact$",
      "class-name": "TreeSitterTact"
    }
  ],
  "metadata": {
    "version": "1.5.2",
    "license": "MIT",
    "description": "A tree-sitter grammar for the Tact programming language",
    "authors": [
      {
        "name": "Novus Nota",
        "url": "https://github.com/novusnota"
      }
    ],
    "links": {
      "repository": "https://github.com/tact-lang/tree-sitter-tact"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
