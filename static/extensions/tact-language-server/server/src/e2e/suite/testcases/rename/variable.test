Rename parameter
========================================================================
fun test(value: Int) {
    //!  ^ age
    dump(value);
}
------------------------------------------------------------------------
fun test(age: Int) {
    //!  ^ age
    dump(age);
}

========================================================================
Rename parameters
========================================================================
fun test(
    value: Int,
//! ^ age
    value2: String,
//! ^ name
    value3: Address,
//! ^ owner
) {
    dump(value);
    dump(value2);
    dump(value3);
}
------------------------------------------------------------------------
fun test(
    age: Int,
//! ^ age
    name: String,
//! ^ name
    owner: Address,
//! ^ owner
) {
    dump(age);
    dump(name);
    dump(owner);
}
