message Some {}

/// Some cool contract
contract <caret>Foo {
    name: String;
    value: Int;

    init(name: String, value: Int) {
        self.name = name;
        self.value = value;
    }

    receive("hello") {}
    receive(msg: Some) {}
    external(msg: Slice) {}
    bounced(msg: bounced<Some>) {}

    get fun counter(): Int {
        return self.value;
    }
}
